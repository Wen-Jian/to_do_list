<div class="container form">
<%= form_for @list, :url => lists_path, :method => :post do |f| %>
	<div class = "form-group">
		<%= f.label :title %>
		<%= f.text_field :title, class: "form-control" %>
	</div>
	<div class = "form-group">
		<%= f.label :due_date %>
		<%= f.text_field :due, class: "form-control" %>
	</div>
	<div class = "form-group">
		<%= f.label :notes %>
		<%= f.text_area :notes, class: "form-control" %>
	</div>

	<div>
		<a href= <%= lists_path %>><button>cancel</button></a>
		<%= f.submit "Creat" %>
	</div>
<% end %>	
</div>


